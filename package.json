{
  "name": "react-graphql-apollo-tutorial-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js --mode development",
    "start": "webpack-dev-server --mode development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "schema": "apollo schema:download ./src/schema.json --endpoint=https://api.github.com/graphql --header \"Authorization: Bearer sdasdasd",
    "types": "apollo codegen:generate ./src/__generated__/types.ts --addTypename --queries=./src/**/**.graphql.ts --localSchemaFile=./src/schema.json --target=typescript --outputFlat"
  },
  "dependencies": {
    "@apollo/client": "3.1.3",
    "@apollo/link-error": "2.0.0-beta.3",
    "apollo": "^2.32.5",
    "apollo-cache-inmemory": "^1.2.9",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "graphql": "^14.6.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.11.0",
    "react-dom": "16.11.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/recompose": "^0.26.5",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.2",
    "ts-loader": "^5.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.11.0"
  }
}
